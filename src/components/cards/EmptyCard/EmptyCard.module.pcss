.container {
	position: relative;
	overflow: hidden;
	flex: 1;
	align-items: center;
	justify-content: center;
}

.blur {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	filter: blur(10px);
	background-size: cover;
	background-position: center;
	background-repeat: no-repeat;
}

.title {
	z-index: 1;
	max-width: 200px;
	text-align: center;
}
